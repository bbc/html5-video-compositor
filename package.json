    "url": "https://github.com/bbc/html5-video-compositor"
  },
  "contributors": [
    {
      "name": "Matthew Shotton",
      "email": "matthew.shotton@bbc.co.uk"
    },
    {
      "name": "Max Leonard",
      "email": "max.leonard@bbc.co.uk"
    }
